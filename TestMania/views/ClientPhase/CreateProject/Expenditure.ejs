<!DOCTYPE html>
<html lang="en">

<head>

<% include ../Partials/ClientStyles %>

</head>

<body>

    <div id="wrapper">

       <% include ../Partials/ClientNavBars %>

        <div id="page-wrapper" style="height:700px !important;">

            <div class="container-fluid">

                <!-- Page Heading -->
                <div class="row">
                    <div class="col-lg-12">
                        <h1 class="page-header">Create Project 
                            <small>Expenditure</small>
                        </h1>
                       
                    </div>
                </div>
                <!-- /.row -->
				</div>
				<div class="row">
				<% include ../Partials/CreateProjectNavBars %>
				</div>
				<div class="row" style="margin-left:1%;">
					<div class="col-lg-3">

                        <form role="form">
			
                            <div class="form-group">
							    <label>No. Devices Selected:</label>
                            </div>
							 <div class="form-group">
                                <label>Estimated Cost:</label>
                             </div>
							 <div class="form-group">
                                <label>Available Credit:</label>
                             </div>
							<div class="form-group">
                                <label>Coupan Code:</label>
                             </div>
							 <div class="form-group">
                                <label>Total:</label>
                             </div>
							 							
						</form>
					</div>	
					<div class="col-lg-4">

                        <form role="form">
			
                            <div class="form-group">
							  <label>12</label>
                            </div>
							<div class="form-group">
							  <label>$</label>
							  <label>100.00</label>
                            </div>
							<div class="form-group">
							  <label>$</label>
							  <label>50.0</label>
                            </div>
							<div class="form-group">
							  <input class="form-control">
							</div>
							<div class="form-group" style="color: red;">
							  <label>$</label>
							  <label>40.0</label>
							</div>
							<div class="form-group">
							  <button type="button" class="btn btn-primary" id="confirmCreateProjBtn">Confirm</button>
							</div>
						</form>
					</div>
						<div class="col-lg-3">

                        <form role="form">
			
                            <div class="form-group" style="margin-top:53%;">
							   <button type="button" class="btn btn-primary" id="applyCoupanBtn">Apply</button>
							</div>
							<div class="form-group" style="margin-top:-4%;">
							   <button type="button" class="btn btn-primary" id="payNowBtn">Pay Now</button>
							</div>
							
							 
						</form>
					</div>
            <!-- /.container-fluid -->

        </div>
        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->

	<!-- Pay Now PopUp -->
	<div id="payNowModal" class="modal fade payNowModal" role="dialog" >
		<div class="modal-dialog">

    <!-- Modal content-->
			<div class="modal-content">
				<div class="modal-header payNowModalHeader">
					<button type="button" class="close" data-dismiss="modal">&times;</button>
					<h4 class="modal-title">Pay Now</h4>
				</div>
			<div class="modal-body" style="overflow-y:auto;">
                	<div class="col-lg-3">

                        <form role="form">
			
                            <div class="form-group">
							    <label class="popupAddDeviceLabel">Card Type:</label>
                            </div>
							 <div class="form-group">
                                <label class="popupAddDeviceLabel">Account No:</label>
                             </div>
							 <div class="form-group">
                                <label class="popupAddDeviceLabel">CVV No:</label>
                             </div>
							 <div class="form-group">
                                <label class="popupAddDeviceLabel">Amount:</label>
                             </div>
							
							
						</form>
					</div>
					<div class="col-lg-6">

                        <form role="form">
			
                            <div class="form-group">
							   <input class="form-control">
							</div>
							<div class="form-group">
							    <input class="form-control">
							</div>
							<div class="form-group">
							    <input class="form-control" style="width:30%;">
							</div>
							<div class="form-group" style="color:red;">
							    <label>$</label>
								<label>40.00</label>
							</div>
							
						</form>
				</div>
			
			</div>
		<div class="modal-footer">
		    <button type="button" class="btn btn-primary" id="paySubmitBtn">Pay</button>
			<button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>
		</div>
    </div>

  </div>
</div>
	<!-- /Pay Now PopUp -->
	
	
	
    <!-- jQuery -->
    <script src="js/jquery.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="js/bootstrap.min.js"></script>

    <!-- Morris Charts JavaScript -->
    <script src="js/plugins/morris/raphael.min.js"></script>
    <script src="js/plugins/morris/morris.min.js"></script>
    <script src="js/plugins/morris/morris-data.js"></script>
	 <script src="js/jquery-1.9.1.min.js"></script>
	 <script src="js/jquery-1.11.3.min.js"></script>
	 <script src="js/bootstrap-3.3.5.min.js"></script>
	<script>
	$(document).ready(function(){
		$("#payNowBtn").click(function(){
			
			$("#payNowModal").modal();
		});
		
		
	});
	</script>

</body>

</html>
