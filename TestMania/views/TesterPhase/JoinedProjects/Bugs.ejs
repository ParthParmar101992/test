<!DOCTYPE html>
<html lang="en">

<head>

<% include ../Partials/TesterStyles %>

</head>

<body>

    <div id="wrapper">

       <% include ../Partials/TesterNavBars %>

        <div id="page-wrapper" style="height:700px !important;">

            <div class="container-fluid">

                <!-- Page Heading -->
                <div class="row">
                    <div class="col-lg-12">
                        <h1 class="page-header">Joined Projects 
                            <small>Bugs</small>
                        </h1>
                       
                    </div>
                </div>
                <!-- /.row -->
				<div class="row">
				<% include ../Partials/JoinedProjectsNavBars %>
				</div>
							
				<div class="row" style="float:right;margin-right: 10%;">
				    <div class="col-lg-6"><button type="button" class="btn btn-primary" id="backToJoinedProjectsBtn">Back</button></div>
				</div>
				<div class="row" style="margin-top:4%;margin-left: 3%;">
                    <div class="col-lg-8">
						<div class="well well-sm replyAnnounce outerReply">
							<div class="headerReply">
							    <div class="headerNameReply">Parth Parmar</div>
								<div class="headerTimeReply">6 months ago</div>
							</div>
							<div class="contentReply">
								Let me answer my own question js/custom.js and change the mailchimp list link. Very nicely done. Thank you for the template!
							</div>
						
						</div>
						<div class="bugsBtns">
						 <label>Status: Approved</label>
						 <button type="button" class="btn btn-xs btn-default" id="replyBug1Btn" style="margin-left: 75%;margin-top:-20%">Reply</button>
						</div>
						
						<div class="well well-sm replyAnnounce outerReply">
							<div class="headerReply">
							    <div class="headerNameReply">John Smith</div>
								<div class="headerTimeReply">8 months ago</div>
							</div>
							<div class="contentReply">
								:D Thanks for purchasing the template. :)
							</div>
						
						</div>
						<div class="bugsBtns">
						<label>Status: Reported</label>
						 <button type="button" class="btn btn-xs btn-default" id="replyBug2Btn" style="margin-left:75%;margin-top:-20%">Reply</button>
						</div>
					</div>
				</div>
				<div class="row" style="margin-top:2%;margin-left: 3%;">
                    <div class="col-lg-8">
					     <form role="form">
						  <div class="form-group">
                                <textarea class="form-control" rows="3"></textarea>
                            </div>
						</form>
					</div>
					<div class="col-lg-3">
					      <button type="button" class="btn btn-xs btn-default" id="reportBugBtn" >Report Bug</button>
					</div>
				</div>
			</div>
            <!-- /.container-fluid -->

        </div>
        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->
	
	<!-- Reply PopUp -->
	<div id="replyModal" class="modal fade replyModal" role="dialog" >
		<div class="modal-dialog">

    <!-- Modal content-->
			<div class="modal-content">
				<div class="modal-header replyModalHeader">
					<button type="button" class="close" data-dismiss="modal">&times;</button>
					<h4 class="modal-title">Annoncement Reply</h4>
				</div>
			<div class="modal-body">
                <form role="form">
					<div class="form-group">
                         <textarea class="form-control" rows="3"></textarea>
                    </div>
				</form>
			</div>
		<div class="modal-footer">
		    <button type="button" class="btn btn-primary" id="replySubmitBtn">Reply</button>
			<button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>
		</div>
    </div>

  </div>
</div>
	<!-- /Reply PopUp -->
	
    <!-- jQuery -->
    <script src="/js/jquery.js"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="/js/bootstrap.min.js"></script>
    <!-- Morris Charts JavaScript -->
    <script src="/js/plugins/morris/raphael.min.js"></script>
    <script src="/js/plugins/morris/morris.min.js"></script>
    <script src="/js/plugins/morris/morris-data.js"></script>
	 <script src="/js/jquery-1.9.1.min.js"></script>
	  <script src="/js/jquery-1.11.3.min.js"></script>
	   <script src="/js/bootstrap-3.3.5.min.js"></script>
	<script>
	$(document).ready(function(){
		$("#replyBug1Btn").click(function(){
			$("#replyModal").modal();
		});
		$("#backToJoinedProjectsBtn").click(function(){
			window.location.href = "http://localhost:1926/TesterPhase/JoinedProjects";
		});
	});
	</script>
</body>
</html>
